{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "name": "@sojoorn/angular2-multiselect-dropdown",
  "version":"5.0.8",
  "license": "MIT",
  "author": "Pradeep Terli",
  "description": "Angular Multiselct dropdown component library for Angular version 11, 10, 9,8,6,4 ",
  "keywords": [
    "angular 2 multiselect dropdown",
    "angular 4 multiselect dropdown",
    "angular multiselect",
    "angular dropdown",
    "angular 6 dropdown multiselect"
  ],
  "ngPackage": {
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "allowedNonPeerDependencies": ["@tweenjs/tween.js"],
    "assets": [
      "README.md",
      "LICENSE",
      "./themes/*.theme.scss",
      "./themes/*.theme.css"
    ],
    "dest": "../../dist"
  },
  "peerDependencies": {
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.4.0"
  },
  "private": false,
  "repository": {
    "url": "git://github.com/Sojoorn/angular2-multiselect-dropdown.git"
  },
  "bugs": {
    "url": "https://github.com/Sojoorn/angular2-multiselect-dropdown/issues"
  }
}